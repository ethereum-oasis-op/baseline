  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install",
    "build:all": "cp .env.localdev .env && npm-run-all build:didserver build:next clean lint tsc",
    "build": "npm-run-all clean lint tsc",
    "build:next": "next build",
    "build:didserver": "sh ./dafinstall.sh",
    "start:didserver": "node did/build/index.js daf server",
    "dev": "nodemon --watch src -e ts,ejs,js --exec npm-run-all build start start:didserver",
    "start": "node . & next dev -H 0.0.0.0",
    "prod": "npm-run-all build build:next start:prod",
    "start:prod": "cp ecosystem.config.js dist/ && pm2 start dist/ecosystem.config.js --no-daemon",
    "test": "NODE_ENV=test jest --runInBand --verbose --detectOpenHandles --forceExit",
    "up:besu": "docker-compose up -d alice-mongo alice-besu",
    "up:ganache": "sudo docker-compose up -d alice-mongo ganache",
    "up:infura": "sudo docker-compose up -d alice-mongo",
    "down": "sudo docker-compose down",
    "build:tailwind": "tailwind build assets/styles/index.css -o assets/styles/tailwind.css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "optionalDependencies": {
    "@types/googlemaps": "3.39.13",
    "@types/markerclustererplus": "2.1.33",
    "@types/react": "16.9.49"
  }
