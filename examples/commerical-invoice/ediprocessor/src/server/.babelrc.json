{
  "sourceMap": true,
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": true
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "common-services": "./src/server/Services",
          "common-core": "./src/server/Core",
          "common-custom": "../../other"
        }
      }
    ],
    "@babel/plugin-transform-async-to-generator"
  ]
}
